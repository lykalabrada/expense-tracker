  "bugs": {
    "url": "https://github.com/lykalabrada/expense-tracker/issues"
  },
  "homepage": "https://github.com/lykalabrada/expense-tracker#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "engines": {
    "node": "11.11.0",
    "npm": "6.7.0"
  }
